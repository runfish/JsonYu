package com.xiaoyu.test;

public class InnerClassDemo4 {
	
	public static void main(String[] args) {
		
	}

}

class Outer4{
	int num = 32;
	class Inner{
		
		void show(){
			System.out.println("   ....."+num);
		}
	}
	public void method() {
		
		Inner inner = new Inner();
		
		inner.show();
	}
	
}
