package com.xiaoyu.test;

public class InnerClassDemo3 {
	
	public static void main(String[] args) {
		new Outer3().method();
	}

}

class Outer3{
	
 int num =3;
	
	void method(){
		
		final int x = 12;
	class Inner{
		
		void show(){
			System.out.println("方法里面的内部类"+x);
		}
		
	}
	Inner inner = new Inner();
	inner.show();
	
		
	}
	
	
}
