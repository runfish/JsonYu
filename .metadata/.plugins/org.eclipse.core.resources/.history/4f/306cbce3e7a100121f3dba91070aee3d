package com.xiaoyu.test;



public class InnerClass {

	public static void main(String[] args) {
		Outer outer = new Outer();
		outer.getInner();
	}
}

class Outer{
	
	private int num = 10;
	
	class Inner{
		void show(){
			System.out.println("内部类方法...."+num);
		}
	}
	
	public void getInner() {
		Inner inner = new Inner();
		inner.show();
	}
	
}
